sudo usermod -aG docker <username>
docker build -t <image name to be >:<tag id to be> .
docker build -t <image name to be>:<tag id to be> <Dockerfile path> 
docker images
docker rmi <images id>
docker rmi <image name>:<Version>
docker run -d --name <container name to be > <image name>:<tagid>
docker run -d --name <container name to be > -p 80:80 <image name>:<tagid>
docker ps
docker ps -a
docker start <container Name/id>
docker stop <Container Name/id>
docker rm <container Name/id>
docker logs <container Name/id>
docker logs -f <Container Name/id>
docker inspect <imageid>
docker inspect <imagname>:<tag id>
docker inspect <container id>
docker inspect <container name>
docker volume ls
